{#{% extends "_template.html.twig" %}#}

{% block title %}
    <h2 class="title is-padding-10 has-text-centered"> Nos specialités pour vos événements</h2>
{% endblock %}

{% block content %}

    {% set compt = 0 %}

    <div class="container menu-container">
        {% for category in categoryList %} {# @var category App/Entity/Category #}

            <article class="menu menu-box box is-padding-0 is-vauto">
                <div class="menu-list">
                    <div class=" menu-list-photo">
                        <img src="{{ asset('images/'~category.photo) }}" alt="">
                    </div>

                    <a class="is-padding-0 " href="{{ path('menu_list',{id:category.id}) }}">

                        <div class=" menu-list-p-title is-margin-10">
                            <h3 class="">
                                <strong>{{ category.titre }}</strong>
                            </h3>
                            <h3 class="is-size-7">Découvrez notre sélection</h3>
                        </div>

                    </a>
                </div>

                <div class="contenu has-text-justified">
                    <p class="">
                        <small>{{ category.descriptioncourte }}</small>
                    </p>
                </div>
            </article>
        {% endfor %}

    </div>
{% endblock %}